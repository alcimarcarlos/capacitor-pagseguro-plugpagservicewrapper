{
  "name": "capacitor-pagseguro-plugpagservicewrapper",
  "version": "0.0.1",
  "description": "PlugPagServiceWrapper é um plugin para o Capacitor da biblioteca para integrar aplicativos Android com o terminal Moderninha Smart do PagSeguro e utilizar a solução de pagamentos, PlugPag, embarcado no POS.",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Alcimar Carlos de L V e Aires",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^2.4.9",
    "@capacitor/core": "^2.4.9",
    "@capacitor/ios": "^2.4.9",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.56.3",
    "swiftlint": "^1.0.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {},
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorPagseguroPlugpagservicewrapper.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/UsinaTech/capacitor-pagseguro-plugpagservicewrapper.git"
  },
  "bugs": {
    "url": "https://github.com/UsinaTech/capacitor-pagseguro-plugpagservicewrapper.git/issues"
  },
  "dependencies": {}
}
